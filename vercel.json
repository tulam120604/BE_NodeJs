{
    "version": 2,
    "builds": [
        {
            "src": "./src/app.js",
            "use": "@vercel/build-node",
             "config" : {
                "module" : "esm"
             }
        }
    ],
    "routes" : [
        {
            "src" : "/(.*)",
            "dest" : "./src/app.js"
        }
    ]
}